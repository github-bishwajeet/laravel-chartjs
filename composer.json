{
    "name": "jeet/laravelchartjs",
    "description": "Simple package to facilitate and automate the use of charts in Laravel 5.x using Chartjs v2 library",
    "keywords": [
      "chartjs","laravel5","chart","reports","graphics", "jeet"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bishwajeet Kumar",
            "email": "bishwajeetk003@gmail.com"
        }
    ],
    "require": {
      "php": ">=5.6.4",
      "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },

    "autoload": {
      "psr-4": {
        "Jeet\\LaravelChartJs\\" : "src/"
      }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Jeet\\LaravelChartJs\\Providers\\ChartjsServiceProvider"
            ]
        }
    }
}
